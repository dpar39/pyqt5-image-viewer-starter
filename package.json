{
    "name": "pyqt-opencv-starter",
    "description": "PyQt/OpenCV starter template",
    "author": "Darien Pardinas",
    "license": "MIT",
    "version": "1.0.0",
    "scripts": {
        "setup-env": "pip install virtualenv && python -m virtualenv env && call env\\Scripts\\activate.bat && pip install -r requirements-frozen.txt",
        "generate-ui": "python gui/runpyuic.py",
        "start": "npm run build-ui && python main.py", 
        "build-pyds": "python setup.py build_ext --inplace",
        "build-exe": "npm run build-pyds && pyinstaller.exe pyqt5demo-pyd.spec"
    }
}